@{
    ViewBag.Title = "Kalendar erstellen";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h2>Create</h2>

<p>
    name
</p>

<p>
    <input type="text" data-bind="textInput: name"/>
</p>

<p>
    isPrivate
</p>

<p>
    <input type="checkbox" data-bind="checked: isPrivate"/>
</p>

<button class="btn btn-lg btn-primary btn-block btn-signin" data-bind="click: create">Create</button>

@section scripts {
    <script src="~/Scripts/CreateCalendar.js"></script>
}